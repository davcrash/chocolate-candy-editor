{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["import"],
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "requireConfigFile": false,
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error"
  }
}
